#include <iostream>
#include<vector>
using namespace std;

int main() {
	int x,count=0;
	cin>>x;
	vector <int> v;
	for(int i=1;i<=x;i++)
	{
	    if(x%i==0)
	    {
	        count++;
	        v.push_back(i);
	    }
	}
	cout<<count<<endl;
	for(int i=0;i<v.size();i++)
	{
	    cout<<v[i]<<" ";
	}
	return 0;
}
